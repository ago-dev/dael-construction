# Sanity Configuration
# REQUIRED: Your Sanity project ID (previously hardcoded as 'ejx17wi7')
NEXT_PUBLIC_SANITY_PROJECT_ID=your_project_id

# Default dataset is 'production' if not specified
NEXT_PUBLIC_SANITY_DATASET=production

# API version (default: 2023-05-03)
NEXT_PUBLIC_SANITY_API_VERSION=2023-05-03

# Required for CMS content management
SANITY_STUDIO_API_TOKEN=your_token_here

# Optional: URL for the Sanity CDN (for images)
NEXT_PUBLIC_SANITY_IMAGE_BASE_URL=https://cdn.sanity.io/images/

# Email configuration for contact form
EMAIL_USER=your_gmail_email@gmail.com
EMAIL_PASS=your_app_password
ADMIN_EMAIL=recipient_email@example.com 